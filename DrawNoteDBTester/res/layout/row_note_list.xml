<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_margin="5dp"
	android:orientation="horizontal" >

	<TextView
		android:id="@+id/row_note_id"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:layout_margin="5dp"
		android:gravity="center"
		android:text="id" />

	<TextView
		android:id="@+id/row_note_name"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:layout_margin="5dp"
		android:gravity="center"
		android:text="name" />

	<TextView
		android:id="@+id/row_note_create_date"
		android:layout_width="0dp"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:gravity="center"
		android:text="create date" />

	<TextView
		android:id="@+id/row_note_update_date"
		android:layout_width="0dp"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:gravity="center"
		android:text="update date" />

	<TextView
		android:id="@+id/row_note_page_count"
		android:layout_width="wrap_content"
		android:layout_height="match_parent"
		android:layout_margin="5dp"
		android:gravity="center"
		android:text="0" />

</LinearLayout>